{
  "name": "express-mongo-ts-template",
  "version": "1.0.0",
  "description": "API Template using Express, Mongo and TypeScript",
  "main": "app.js",
  "scripts": {
    "dev": "NODE_ENV=staging ./node_modules/.bin/gulp dev",
    "build": "./node_modules/.bin/gulp build./dockerbuild.sh",
    "deploy:up": "ssh root@rolistudio.com \"docker-compose up -d\"",
    "deploy:down": "ssh root@rolistudio.com \"docker-compose down\"",
    "deploy:update": "ssh root@rolistudio.com \"docker-compose pulldocker-compose up -d\""
  },
  "author": "Rohith Balaji",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.716.0",
    "bcrypt": "^3.0.8",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.5",
    "custom-env": "^1.0.2",
    "debug": "^4.1.1",
    "express": "^4.18.2",
    "express-jwt": "^6.0.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.25",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/express-jwt": "0.0.42",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.32",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^13.13.14",
    "@types/ws": "^6.0.4",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  }
